<view class="container">
  <view class="quote-container">
    <view class="mbti-type">您的MBTI类型：{{mbtiType}}</view>
    <view class="time-message">{{timeMessage}}</view>
    <view class="quote-text">"{{quote}}"</view>
    
    <view class="button-group">
      <button class="refresh-btn" bindtap="refreshQuote">换一条</button>
      <button class="home-btn" bindtap="goHome">返回首页</button>
    </view>
  </view>
</view> 